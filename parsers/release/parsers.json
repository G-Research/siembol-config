{
  "parsers_version": 5,
  "parsers_configurations": [
    {
      "parser_name": "aws_cloudtrail",
      "parser_version": 3,
      "parser_author": "siembol",
      "parser_description": "AWS cloudtrail parser.",
      "parser_attributes": {
        "parser_type": "generic"
      },
      "parser_extractors": [
        {
          "name": "json",
          "field": "original_string",
          "post_processing_functions": [
            "format_timestamp"
          ],
          "extractor_type": "json_extractor",
          "attributes": {
            "should_overwrite_fields": true,
            "should_remove_field": false,
            "remove_quotes": true,
            "skip_empty_values": false,
            "thrown_exception_on_error": false,
            "nested_separator": "_",
            "time_formats": [
              {
                "time_format": "yyyy-MM-dd'T'HH:mm:ss'Z'",
                "timezone": "UTC"
              }
            ],
            "timestamp_field": "eventTime"
          }
        }
      ],
      "transformations": [
        {
          "transformation_type": "delete_fields",
          "attributes": {
            "fields_filter": {
              "including_fields": [
                "^requestParameters_resource.*$"
              ]
            }
          }
        }
      ]
    },
    {
      "parser_name": "win_eventlogs",
      "parser_version": 2,
      "parser_author": "siembol",
      "parser_description": "Parser for win eventlogs sent in syslog format.",
      "parser_attributes": {
        "parser_type": "syslog",
        "syslog_config": {
          "syslog_version": "RFC_5424",
          "merge_sd_elements": false,
          "timezone": "UTC"
        }
      },
      "parser_extractors": [
        {
          "name": "kv_extractor",
          "field": "original_string",
          "extractor_type": "key_value_extractor",
          "attributes": {
            "should_overwrite_fields": true,
            "should_remove_field": false,
            "remove_quotes": true,
            "skip_empty_values": false,
            "thrown_exception_on_error": false,
            "word_delimiter": " ",
            "key_value_delimiter": "=",
            "escaped_character": "\\",
            "quota_value_handling": true,
            "next_key_strategy": false,
            "escaping_handling": true,
            "rename_duplicate_keys": false
          }
        }
      ]
    },
    {
      "parser_name": "siembol_heartbeat",
      "parser_version": 1,
      "parser_author": "siembol",
      "parser_description": "Parser for siembol heartbeat",
      "parser_attributes": {
        "parser_type": "generic"
      },
      "parser_extractors": [
        {
          "name": "json",
          "field": "original_string",
          "post_processing_functions": [
            "format_timestamp"
          ],
          "extractor_type": "json_extractor",
          "attributes": {
            "should_overwrite_fields": true,
            "should_remove_field": false,
            "remove_quotes": true,
            "skip_empty_values": false,
            "thrown_exception_on_error": false,
            "nested_separator": ":",
            "time_formats": [
              {
                "time_format": "yyyy-MM-dd'T'HH:mm:ss'Z'",
                "timezone": "UTC"
              }
            ],
            "timestamp_field": "event_time"
          }
        }
      ]
    }
  ]
}